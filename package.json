{
  "name": "jessepiccione-info-frontend",
  "version": "2.4.1",
  "private": true,
  "dependencies": {
    "express": "^4.21.1",
    "http": "^0.0.1-security",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.25.1",
    "three": "^0.134.0",
    "vanta": "^0.5.24",
    "web-vitals": "^2.1.4",
    "ws": "^8.18.0"
  },
  "scripts": {
    "start": "NODE_ENV=production node dist/server.bundle.js",
    "build": "NODE_ENV=production npm install",
    "start-dev": "NODE_ENV=development npm run build-server && NODE_ENV=development node dist/server.bundle.js",
    "install-prod": "NODE_ENV=production npm install --loglevel silly",
    "install-dev": "NODE_ENV=development npm install --loglevel silly",
    "build-server": "react-scripts build && webpack --config webpack.server.config.js",
    "clear": "rm -rf node_modules | rm package-lock.json",
    "deploy": "npm run install-dev && npm run build-server && npm run install-prod && gcloud init --skip-diagnostics && gcloud app deploy"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest":{
    "testEnvironment":"jsdom"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-private-property-in-object": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "babel-loader": "^9.2.1",
    "null-loader": "^4.0.1",
    "react-scripts": "^5.0.1",
    "terser-webpack-plugin": "^5.3.10",
    "typescript": "^4.9.5",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
